<launch>
    <include file="$(find-pkg-share autoware_carla_interface)/autoware_carla_interface.launch.xml">
        <arg name="host" value="localhost"/> 
        <arg name="port" value="2000"/> 
        <arg name="timeout" value="20"/> 
        <arg name="ego_vehicle_role_name" value="ego_vehicle"/> 
        <arg name="vehicle_type" value="vehicle.toyota.prius"/> 
        <arg name="spawn_point" value="None"/> 
        <arg name="carla_map" value="Town01"/> 
        <arg name="sync_mode" value="True"/> 
        <arg name="fixed_delta_seconds" value="0.05"/> 
        <arg name="objects_definition_file" value="$(find-pkg-share autoware_carla_interface)/objects.json"/> 
        <arg name="use_traffic_manager" value="False"/> 
        <arg name="max_real_delta_seconds" value="0.05"/>
    </include>
    <include file="$(find-pkg-share vm_vehicle_launch)/launch/vehicle_visualizer.launch.xml">
        <arg name="active_rviz" value="true"/> 
        <arg name="rviz_config" value="$(find-pkg-share autoware_carla_interface)/sensing_simulation.rviz"/>
        <arg name="vehicle_model" value="vm_vehicle"/> 
        <arg name="sensor_model" value="vm_sim_sensor_kit"/> 
    </include>
</launch>